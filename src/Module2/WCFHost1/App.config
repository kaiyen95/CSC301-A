<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="authenticated">
					<security mode="TransportCredentialOnly">
						<transport clientCredentialType="Windows" />
					</security>
				</binding>
			</basicHttpBinding>
			<wsHttpBinding>
				<binding name="encrypted">
					<security mode="Message">
						<message algorithmSuite="Basic128"
							clientCredentialType="Windows" />
					</security>
				</binding>
			</wsHttpBinding>
			<netTcpBinding>
				<binding name="encrypted">
					<security mode="Message">
						<message algorithmSuite="Basic128"
							clientCredentialType="Windows" />
					</security>
				</binding>
			</netTcpBinding>
		</bindings>
		<behaviors>
			<serviceBehaviors>
				<behavior name="default">
					<serviceMetadata
						httpGetEnabled="true"
						httpGetUrl="http://locahost:8082/symbank/services"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<services>
			<service name="SymBank.Services.BankingServices"
					 behaviorConfiguration="default">
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8080/symbank/services/"/>
						<add baseAddress="net.tcp://localhost:8081/symbank/services/"/>
					</baseAddresses>
				</host>
				<endpoint
					address="mex"
					binding="mexHttpBinding"
					contract="IMetadataExchange" />
				<endpoint
					name="http_account"
					address="account"
					binding="wsHttpBinding"
					bindingConfiguration="encrypted"
					contract="SymBank.Services.IAccountService" />
				<endpoint
					name="http_transaction"
					address="transaction"
					binding="wsHttpBinding"
					bindingConfiguration="encrypted"
					contract="SymBank.Services.ITransactionService" />
				<endpoint
					name="tcp_account"
					address="account"
					binding="netTcpBinding"
					bindingConfiguration="encrypted"
					contract="SymBank.Services.IAccountService" />
				<endpoint
					name="tcp_transaction"
					address="transaction"
					binding="netTcpBinding"
					bindingConfiguration="encrypted"
					contract="SymBank.Services.ITransactionService" />
			</service>
		</services>
	</system.serviceModel>
</configuration>