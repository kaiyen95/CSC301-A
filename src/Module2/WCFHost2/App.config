<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<system.serviceModel>
		<behaviors>
			<serviceBehaviors>
				<behavior name="default">
					<serviceMetadata
						httpGetEnabled="true"
						httpGetUrl="http://locahost:8082/symbank/services"/>
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<services>
			<service name="SymBank.Services.BankingServices"
					 behaviorConfiguration="default">
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8080/symbank/services/"/>
						<add baseAddress="net.tcp://localhost:8081/symbank/services/"/>
					</baseAddresses>
				</host>
				<endpoint
					address="mex"
					binding="mexHttpBinding"
					contract="IMetadataExchange" />
				<endpoint
					name="http_account"
					address="account"
					binding="basicHttpBinding"
					contract="SymBank.Services.IAccountService" />
				<endpoint
					name="http_transaction"
					address="transaction"
					binding="basicHttpBinding"
					contract="SymBank.Services.ITransactionService" />
				<endpoint
					name="tcp_account"
					address="account"
					binding="netTcpBinding"
					contract="SymBank.Services.IAccountService" />
				<endpoint
					name="tcp_transaction"
					address="transaction"
					binding="netTcpBinding"
					contract="SymBank.Services.ITransactionService" />
			</service>
		</services>
	</system.serviceModel>
</configuration>